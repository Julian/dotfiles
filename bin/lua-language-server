#!/usr/bin/env python3
from pathlib import Path
import os
import sys


_LLS = Path(os.environ["DEVELOPMENT"]) / "lua-language-server"
_BIN = _LLS / "bin"
executable, = _BIN.glob("*/lua-language-server")
argv = [str(executable), "-E", str(_LLS / "main.lua")] + sys.argv[1:]
os.execvp(executable, argv)
